{
  "name": "@css-picons/config",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Pure CSS icons for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Abraham - anubra266 <anubra266@gmail.com>",
  "scripts": {
    "build": "tsup src/index.ts --format=cjs,esm --shims --dts",
    "build-fast": "tsup src/index.ts --format=cjs,esm --shims --no-dts",
    "dev": "pnpm build-fast --watch",
    "test": "echo \"Error: no test specified\""
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@css-picons/types": "workspace:*",
    "@css-picons/variants": "workspace:*"
  },
  "devDependencies": {
    "@pandacss/dev": "dev",
    "@pandacss/types": "dev"
  }
}